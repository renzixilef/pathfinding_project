{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Pathfinding (GUI)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Pathfinding_Bench_Program",
      "args": ["--gui"],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        { "description": "Enable pretty-printing", "text": "-enable-pretty-printing", "ignoreFailures": true }
      ],
      "preLaunchTask": "CMake: build"
    }
  ]
}